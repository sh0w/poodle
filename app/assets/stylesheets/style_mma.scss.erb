@font-face {
    font-family: 'ballparkweiner';
    src: url('/fonts/ballw___-webfont.eot');
    src: url('/fonts/ballw___-webfont.eot?#iefix') format('embedded-opentype'),
         url('/fonts/ballw___-webfont.woff') format('woff'),
         url('/fonts/ballw___-webfont.ttf') format('truetype'),
         url('/fonts/ballw___-webfont.svg#ballparkweiner') format('svg');
    font-weight: normal;
    font-style: normal;

}

@charset "UTF-8";
html, body, div, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, em, img, ins, kbd, q, s, samp,
small, strike, strong, sub, sup, tt, var,
b, u, i, center,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, embed, 
figure, figcaption, footer, header, hgroup, 
menu, nav, output, ruby, section, summary,
time, mark, audio, video {
	margin: 0;
	padding: 0;
	border: 0;
	font-size: 100%;
	font: inherit;
	vertical-align: baseline;
}

* {
    margin: 0;
}

html, body {
    height:100%;
}

.content-wrapper {
    min-height: 100%;
    height: auto !important;
    height: 100%;
    margin: 0 auto -270px;
}

h1 {
    font-size: 1.5em;
}
h2 {
    font-size: 1.3em;
}
/* HTML5 display-role reset for older browsers */
article, aside, details, figcaption, figure, 
footer, header, hgroup, menu, nav, section {
	display: block;
}
body {
	line-height: 1;

}
ol, ul {
	list-style: none;
}
blockquote, q {
	quotes: none;
}
blockquote:before, blockquote:after,
q:before, q:after {
	content: '';
	content: none;
}
table {
	border-collapse: collapse;
	border-spacing: 0;
}
.clearfix:before,
.clearfix:after {
  content: ".";    
  display: block;    
  height: 0;    
  overflow: hidden;	
}
.clearfix:after {clear: both;}
.clearfix {zoom: 1;} /* IE < 8 */

/* Ich bin mir fast sicher das bis hierher nur ein reset-skript ist, jetzt gehts also los */

/*allgemeiner Schmees, font-size 100% is wichtig für die rems, also ned willkürlich ändern, legt schrift auf 16px fest, falls wirs vergessen sollten*/
html{
	font-size:100%;
	line-height:100%;
	line-height:14px;
	font-size:14px;
	font-family:helvetica, arial, sans-serif;
}

body {
       background-image:url(<%= asset_path 'img/p_bg.jpg' %>);
    background-repeat:repeat;
}
a{
	text-decoration:none; 
	outline:none;
}
/* Menüleiste!*/
header li	{
	float:left;
	width:20%;
	text-align:center;
	color:white;
	text-transform:uppercase;
	margin-top: 20px;
	font-size:1rem; /*16px*/
}

header.main{
	height:60px;
	background:#12857C;
}
	
/*allgemeines layout oder zumindest der versuch eines herzustellen*/	

hr	{height:3px;
	border:0;
	background-image:url(<%= asset_path 'img/divider.jpg' %>);
	background-repeat:repeat-x;

}

#intro, #overview, #comments{
	margin-top:40px;
}

#overview	{}
#overview header h1, #comments header h1	{
	color:#169990; 
	height:60px;
	text-align:center;

}

	/*				KURSBESCHREIBUNG				*/
#intro{
	max-width: 940px;
	width: 90%;
	margin: 0 auto;

	font-size:1.071rem; /*15px*/
	color:#7a7a7a;
	text-shadow: 0px 1px 0px rgba(255, 255, 255, 1);
	line-height:1.714rem;}


#intro header	{
	z-index:5;
	height:118px;
	width:933px;
    background-image:url(<%= asset_path 'img/intro_header.png' %>);
	background-repeat:no-repeat;
	background-position:center;
    left: -60px;
    position: relative;

}
	
#intro header h1{
	text-align:center;
	font-size: 1.571rem; /* 22px */
	text-transform:uppercase;
	color:white;
	padding-top:80px;
	text-shadow: 0px 1px 0px rgba(0, 0, 0, 0.5);
}
#intro_content	{
	z-index:4;
	height:372px;
	width:813px;
	padding-top:50px;
	margin-left:auto;
	margin-right:auto;
    background-image:url(<%= asset_path 'img/intro_content_bg.png' %>);
	background-repeat:no-repeat;
}


#intro_content figure	{
	width:240px;
	margin-left:50px;
	float:left;}
	
#intro_content figure img.rating_bar	{ 
	z-index:100; 
	margin-left:-7px; 
	margin-top:115px;
	position:absolute;}

#intro_content figure img.course_pic	{
	z-index:2; 
	border:2px solid #cecece;}

#intro_text	{
	width:440px;
	height:200px;
	margin-left:30px;
	float:left;}
	
.creator	{
	font-weight:bold;
	text-shadow: 0px 1px 0px rgba(255, 255, 255, 0.24);
}

.course_overview #intro_text p:first-child	{
	font-size:0.8571rem;/*12px*/
	margin-top:-7px;}
	
#intro_text p+p	{
	padding-top:14px;}
	
#progress	{float:left;margin-left:51px;width: 238px;}

#progress	h1{
	text-transform:none;
	font-size:0.7857rem; /*11px*/
	color:#30333c;
	font-weight:bold;
	text-shadow: 0px 1px 0px rgba(255, 255, 255, 0.24);}

#share	{
	float:left;
	margin-left:350px;}
	
#share h1	{
	text-transform:none;
	font-size:0.7857rem; /*11px*/
	color:#30333c;
	font-weight:bold;
	text-shadow: 0px 1px 0px rgba(255, 255, 255, 0.24);
	}


h1.main, h1.main	{
	font-family: 'ballparkweiner';
	font-size: 2.857rem; /* 40px */
	text-shadow: 0px 1px 0px rgba(255, 255, 255, 1);
	color:#169990;
    text-align: center;
}


	
	/*				OVERVIEW				*/

	
#lessons .lesson{
	float:left;
	color:#30333c;
    -moz-border-radius: 2px;
    -webkit-border-radius: 2px;
    border-radius: 2px;
	font-size: 1rem; /* 14px */
	line-height:1.429rem;
    height: 142px;
    overflow: hidden;
    padding: 15px;

	}

.completed	{
    background-image:url(<%= asset_path 'img/grey.png' %>);
	background-repeat:no-repeat;
	}
.current	{
    background-image:url(<%= asset_path 'img/green.png' %>);
	background-repeat:no-repeat;}

.locked {
background-image:url(<%= asset_path 'img/sea.png' %>);
background-repeat:no-repeat;}

.completed h2, .current h2, .locked h2	{font-size: 18px;}

#lessons{
	max-width: 900px;
	width: 90%;
	margin:60px auto;
    text-shadow: 0px 1px 0px rgba(255, 255, 255, 0.25); /*cross browser ....!!*/
}

#lessons h2{
	font-weight: bold;
	margin: 0px;
	}
#lessons p{
	margin: 10px 10px 10px 0px;
}


	/*				KOMMENTARE				*/
	
	
	
#comments	{
	clear:both;
	padding-bottom: 60px;
	padding-top: 90px;
}

#commentbox {
	
	max-width: 900px;
	width: 90%;
	margin:0 auto;
	margin-top: 40px; /*abstand kommentarbox zur überschrift*/
}
.user_thumbnail	{
	float:left;
	padding-bottom:50px;
	margin-right:12px;
	}
.comment {
	width: 250px;
    margin: 5px;
	float:left;
    background-image:url(<%= asset_path 'img/comment.png' %>);
	background-repeat:repeat;
    background-position: left center;
	
}
.comment_content	{
	width:210px;
	margin:13px 11px 25px 28px;;
	font-size:0.8571rem;/*12px*/ line-height:1.429rem;
	color:#787575;
	text-shadow: 0px 1px 1px rgba(255, 255, 255, 0.76);}

.comment header	{
	margin-bottom:4px;}
.when	{
	font-size:0.7143rem;
}
.info	{
	line-height:0.8571rem;
	padding-bottom:10px;
    height: 28px;
}

.info img {
    float:left;
    height: 28px;
    width: 28px;
}
.info .cmt_info {
    float:right;
    width: 175px;
    height: 28px;
    line-height: 14px;
}


.comment .info .cmt_info a	{
    color:#53b292;
    font-size:12px; /*11px*/
    font-weight:bold;
}

.content	{
	line-height:1.071rem;}	
#contact	{
	float:left;
	padding-right:50px;
}


	/*				FOOTER				*/


footer.bottom, .push {
    height: 270px;

}
footer h1{
	text-shadow:none;
	color:white; 
	text-transform:uppercase;
    font-size: 1em;
    padding-bottom: 20px;
}
footer.bottom .links {
    padding-top: 60px;
    text-shadow: 1px 1px 1px #3da29b;
    font-size: 0.9em;
    line-height: 1.2em;
}

footer.bottom a {
    color: #30333c;


}

footer.bottom .container-fluid {
    height: 270px;

}

/*der Versuch das small am Boden festzukleben haben wir aufgegeben, stattdessen hat der HG jetzt die Footerfarbe und das small hängt praktisch am nav*/
small	p {
	font-size:10px;
	font-size:0.625rem;
	text-align:center;
	padding-top:50px;
}

.copyright {
    font-size: 9px;
    height: 10px;
    text-align: center;
}

footer .poodle {
    height: 250px;
}






#popular{
    width: 814px;
    min-height: 1241px;
    margin-left:auto;
    margin-right:auto;
    font-size:1.071rem; /*15px*/
    color:#7a7a7a;
    text-shadow: 0px 1px 0px rgba(255, 255, 255, 1);
    line-height:1.714rem;
    background-image: url(img/search-bg_bottom.png), url(img/search-bg.png);
    background-position: center bottom, left top;
    background-repeat: no-repeat;
    padding-bottom: 80px;

    header	{

        width:877px;
        height:182px;
        background-repeat:no-repeat;
        background-position:center;
        background-image:url(img/ribbon-search.png);
        margin-left:-63px;
    }


    header h1{
        font-size: 1.571rem; /* 22px */
        text-transform:uppercase;
        color:white;
        padding-top: 90px;
        margin-left:120px;
        text-shadow: 0px 1px 0px rgba(0, 0, 0, 0.5);
    }


    #favorites{
        width: 700px;
        margin-left:auto;
        margin-right:auto;
    }
    .course{
        background-color:white;
        width:220px;
        float:left;
        margin-left:10px;
        margin-top:10px;
        webkit-transition: opacity 0.3s linear; -webkit-transition: all 0.3s linear; -moz-transition: all 0.3s linear; transition: all 0.3s linear;
    }
    .course h2{
        font-size:1rem; /*16px*/
        color:black;
        font-weight:bold;
        margin:20px;
    }
    .course aside{
        margin-left: 20px;
    }
    .introtext{
        margin:20px;
        color:black;
        font-size:0.7857rem; /*11px*/
    }
    .course figure img.rating_bar	{
        z-index:100;
        margin-left:-7px;

        position:absolute;}

    .course aside{
        font-size:0.7857rem; /*11px*/
        border-bottom: 1px solid #999999;
        width:120px;
    }


}

#popular:hover {
    .course{
        opacity: 0.7;
        webkit-transition: opacity 0.3s linear; -webkit-transition: all 0.3s linear; -moz-transition: all 0.3s linear; transition: all 0.3s linear;
    }

    .course:hover{
        opacity: 1;
        webkit-transition: opacity 0.3s linear; -webkit-transition: all 0.3s linear; -moz-transition: all 0.3s linear; transition: all 0.3s linear;
    }

}

.edit_user {
    #intro_content {
        padding-bottom: 20px;
        margin-bottom: 30px;
        background-position: bottom;

    }
}